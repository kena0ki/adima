{
  "name": "amida",
  "version": "0.0.0",
  "description": "Amidakuji on the Web",
  "// main": "index.js",
  "scripts": {
    "build:lib": "tsc -p ./src/tsconfig.json",
    "postbuild:lib": "rm -rf ./demo/dist/ && cp -pr ./dist ./demo && cp -pR ./src ./demo/dist/",
    "build:demo": "tsc -p ./demo/tsconfig.json",
    "build": "npm run build:lib && npm run build:demo",
    "watch:lib": "watch -w 1 'npm run build:lib' ./src",
    "prewatch:demo": "npm run build:lib",
    "watch:demo": "watch -w 1 'npm run build:demo' ./demo -p '/build|dist/'",
    "serve": "npx ts-node -P ./script/tsconfig.json ./script/server.ts",
    "// watch:serve": "watch -w 1 'npm run serve' ./script",
    "start": "concurrently -p '{time} {name}' -t 'HH:mm:ss.SSS' -c green,yellow,blue  npm:serve npm:watch:lib npm:watch:demo",
    "test": "echo 'Error: no test specified' && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kena0ki/amida.git"
  },
  "author": "@kena0ki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kena0ki/amida/issues"
  },
  "homepage": "https://github.com/kena0ki/amida#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chokidar": "^3.3.1",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "watch": "kena0ki/watch"
  }
}
